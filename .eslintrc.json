{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "no-unused-expressions": 0,
    "no-undef": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-sequences ": 0,
    "operator-linebreak ": 0,
    "linebreak-style": 0,
    "comma-dangle": 0
  }
}
